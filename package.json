{
  "name": "multilingual-ecommerce-template",
  "version": "1.0.0",
  "description": "A full-featured multilingual e-commerce platform with AI integration",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "fix:chat": "cd frontend && npm run fix:chat",
    "frontend-install": "cd frontend && npm install",
    "backend-install": "cd backend && pip install -r requirements.txt",
    "install-all": "npm run frontend-install && npm run backend-install",
    "dev-all": "concurrently \"npm run dev\" \"cd backend && python -m uvicorn app.main:app --reload\"",
    "test:e2e": "cd frontend && npx playwright test",
    "test:e2e:headed": "cd frontend && npx playwright test --headed",
    "test:e2e:report": "cd frontend && npx playwright show-report",
    "build": "cd frontend && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.0",
    "@playwright/test": "^1.48.0"
  },
  "keywords": [
    "ecommerce",
    "multilingual",
    "rtl",
    "ai",
    "react",
    "fastapi",
    "typescript",
    "persian"
  ],
  "author": "Developer",
  "license": "SEE LICENSE IN LICENSE.txt"
}
